/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "outDir": "./out-tsc/app",
    "types": ["jquery"]
    
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "module": "commonjs",
  "moduleResolution": "node",
  "sourceMap": true,
  "suppressImplicitAnyIndexErrors": true,
  "target": "es5",
  "baseUrl": "src",
  "paths": {
    "@/*": [
        "app/*"
            ]
        }
  
}
